{
  "author": "Victor Algaze @valgaze",
  "bin": {
    "create-speedybot": "./bin/run.js"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "picocolors": "^1.0.0",
    "speedybot": "^2.0.0"
  },
  "description": "Speedybot cli",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.19.10",
    "chai": "^4",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/valgaze/create-speedybot",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "create-speedybot",
  "oclif": {
    "bin": "create-speedybot",
    "dirname": "create-speedybot",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "valgaze/create-speedybot",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "publish:beta": "npm run build && npm publish --tag beta"
  },
  "version": "2.0.6",
  "bugs": "https://github.com/valgaze/create-speedybot/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
